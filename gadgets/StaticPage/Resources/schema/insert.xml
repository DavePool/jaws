<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>

 <name><variable>database</variable></name>

 <table>
  <name><variable>table_prefix</variable>static_pages</name>

  <initialization>
   <insert>
    <field>
      <name>group_id</name>
      <value>1</value>
    </field>

    <field>
      <name>base_language</name>
      <value>en</value>
    </field>

    <field>
      <name>fast_url</name>
      <value>Example</value>
    </field>
    <field>
      <name>show_title</name>
      <value>true</value>
    </field>
    <field>
     <name>updated</name>
     <value><variable>timestamp</variable></value>
    </field>
   </insert>
  </initialization>

 </table>

 <table>
  <name><variable>table_prefix</variable>static_pages_translation</name>

  <initialization>
   <insert>
    <field>
     <name>base_id</name>
     <value>1</value>
    </field>
    <field>
     <name>title</name>
     <value>Example Page</value>
    </field>
    <field>
     <name>content</name>
     <value>This is an example static page, if you would like to change it, or create new pages, you can use the control panel.</value>
    </field>
    <field>
      <name>language</name>
      <value>en</value>
    </field>
    <field>
      <name>published</name>
      <value>true</value>
    </field>
    <field>
     <name>updated</name>
     <value><variable>timestamp</variable></value>
    </field>
   </insert>
  </initialization>

 </table>

 <table>
  <name><variable>table_prefix</variable>static_pages_groups</name>

  <initialization>
   <insert>
    <field>
      <name>title</name>
      <value>General</value>
    </field>

    <field>
      <name>fast_url</name>
      <value>general</value>
    </field>

    <field>
      <name>visible</name>
      <value>true</value>
    </field>

   </insert>
  </initialization>

 </table>

</database>
