<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>
  <name><variable>database</variable></name>

  <table>
    <name><variable>table_prefix</variable>emblem</name>
    <declaration>

      <field>
        <name>id</name>
        <type>integer</type>
        <notnull>true</notnull>
        <autoincrement>true</autoincrement>
      </field>

      <field>
        <name>title</name>
        <type>text</type>
        <length>50</length>
      </field>

      <field>
        <name>src</name>
        <type>text</type>
        <length>255</length>
      </field>

      <field>
        <name>url</name>
        <type>text</type>
        <length>255</length>
      </field>

      <field>
        <name>emblem_type</name>
        <was>type</was>
        <type>text</type>
        <length>1</length>
        <default>P</default>
      </field>

      <field>
        <name>enabled</name>
        <type>boolean</type>
        <default>false</default>
      </field>

      <field>
        <name>updated</name>
        <type>timestamp</type>
      </field>

    </declaration>
  </table>

</database>
